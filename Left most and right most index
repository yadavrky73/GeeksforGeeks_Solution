class Solution:
    def indexes(self, v, x):
        # Your code goes here
        a=[]
        for i in range(len(v)):
            if v[i]==x:
                a.append(i)
        if len(a)==0:
            c=[-1,-1]
            return c
        else:
            c=[a[0],a[len(a)-1]]
            return c
